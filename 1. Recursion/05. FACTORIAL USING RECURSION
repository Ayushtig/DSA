import java.util.*;
public class Recursion {
    static int function(int n){
        if(n==0){
            return 0;
        }
        return n*function(n-1);
    }
    public static void main(String[] args) {
        Scanner sc=new Scanner(System.in);
        System.out.print("enter n: ");
        int n=sc.nextInt();
        sc.close();
        System.out.println(function(n));
    }
}
