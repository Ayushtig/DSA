public class Recursion {
    static int temp=0;
    public static void main(String[] args) {
        int a[]={1,2,3,4,5};
        int n=a.length;
        int i=0;
        int j=n-1;
        while(i<j){
            if(i<j){
            temp=a[i];
            a[i]=a[j];
            a[j]=temp;
            i++;
            j--;
        }}
        for(int k:a){
        System.out.println(k);
    }
    }
}
